# StoryFi Subnet Environment Variables (Google Gemini)
# ======================================================

# Google Gemini API Configuration (推荐)
GEMINI_API_KEY=your-gemini-api-key-here
GEMINI_MODEL=gemini-pro
TEMPERATURE=0.7
MAX_TOKENS=3000

# 或者使用 OpenAI（可选）
# OPENAI_API_KEY=sk-your-openai-api-key-here
# OPENAI_MODEL=gpt-4-turbo-preview

# Bittensor Network Configuration
NETUID=108
WALLET_NAME=my_miner
WALLET_HOTKEY=default

# Validator Configuration (Only for Validators)
VALIDATOR_QUERY_INTERVAL=12
VALIDATOR_TIMEOUT=60
EMA_ALPHA=0.1
SOFTMAX_TEMPERATURE=2.0
PLAGIARISM_THRESHOLD=0.90
BLACKLIST_STRIKES=3

# Logging
LOG_LEVEL=INFO

# Performance
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=60
